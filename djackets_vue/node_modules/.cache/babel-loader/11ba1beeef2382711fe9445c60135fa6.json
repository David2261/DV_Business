<script>\n// @ is an alias to /src\nimport axios from 'axios'\n\nexport default {\n  name: 'Home',\n  data(){\n    return{\n      latestProducts: []\n    }\n  },\n  components: {\n  },\n  methods: {\n    getLatestProducts(){\n      axios\n        .get('api/v1/latest-products/')\n        .then(response => {\n          this.latestProducts = response.data\n        })\n        .catch(error => {\n          console.log(error)\n        })\n    }\n  }\n}\n</script>\n\n<style scoped>\n.image{\n  margin-top: -1.25rem;\n  margin-left: -1.25rem;\n  margin-right: -1.25rem;\n}\n\n</style>"],"sourceRoot":""}]}