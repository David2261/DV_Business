(\"userid\")\r\n            this.$store.commit('removeToken')\r\n            this.$router.push('/')\r\n        },\r\n        async getMyOrders() {\r\n            this.$store.commit('setIsLoading', true)\r\n            await axios\r\n                .get('/api/v1/orders/')\r\n                .then(response => {\r\n                    this.orders = response.data\r\n                })\r\n                .catch(error => {\r\n                    console.log(error)\r\n                })\r\n            this.$store.commit('setIsLoading', false)\r\n        }\r\n    }\r\n}\r\n</script>"],"sourceRoot":""}]}