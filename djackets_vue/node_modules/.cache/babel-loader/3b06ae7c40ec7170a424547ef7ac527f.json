;AAHb,CA7Ba,EAkCb;AACEQ,EAAAA,IAAI,EAAE,OADR;AAEEC,EAAAA,IAAI,EAAE,MAFR;AAGEC,EAAAA,SAAS,EAAET;AAHb,CAlCa,EAuCb;AACEO,EAAAA,IAAI,EAAE,eADR;AAEEC,EAAAA,IAAI,EAAE,SAFR;AAGEC,EAAAA,SAAS,EAAEJ;AAHb,CAvCa,EA4Cb;AACEE,EAAAA,IAAI,EAAE,gBADR;AAEEC,EAAAA,IAAI,EAAE,UAFR;AAGEC,EAAAA,SAAS,EAAEL,QAHb;AAIEM,EAAAA,IAAI,EAAE;AACFC,IAAAA,YAAY,EAAE;AADZ;AAJR,CA5Ca,EAoDb;AACEJ,EAAAA,IAAI,EAAE,+BADR;AAEEC,EAAAA,IAAI,EAAE,SAFR;AAGEC,EAAAA,SAAS,EAAEZ;AAHb,CApDa,EAyDb;AACEU,EAAAA,IAAI,EAAE,iBADR;AAEEC,EAAAA,IAAI,EAAE,UAFR;AAGEC,EAAAA,SAAS,EAAEX;AAHb,CAzDa,CAAf,C,CAgEA;;AACA,IAAMc,MAAM,GAAGnB,YAAY,CAAC;AAC1BoB,EAAAA,OAAO,EAAEnB,gBAAgB,CAACoB,OAAO,CAACC,GAAR,CAAYC,QAAb,CADC;AAE1BV,EAAAA,MAAM,EAANA;AAF0B,CAAD,CAA3B,C,CAKA;;AACAM,MAAM,CAACK,UAAP,CAAkB,UAACC,EAAD,EAAKC,IAAL,EAAWC,IAAX,EAAoB;AACpC,MAAIF,EAAE,CAACG,OAAH,CAAWC,IAAX,CAAgB,UAAAC,MAAM;AAAA,WAAIA,MAAM,CAACb,IAAP,CAAYC,YAAhB;AAAA,GAAtB,KAAuD,CAAChB,KAAK,CAAC6B,KAAN,CAAYC,eAAxE,EAAyF;AACvFL,IAAAA,IAAI,CAAC;AAAEZ,MAAAA,IAAI,EAAE,OAAR;AAAiBkB,MAAAA,KAAK,EAAE;AAAER,QAAAA,EAAE,EAAEA,EAAE,CAACX;AAAT;AAAxB,KAAD,CAAJ;AACD,GAFD,MAEO;AACLa,IAAAA,IAAI;AACL;AACF,CAND;AAQA,eAAeR,MAAf","sourcesContent":["import { createRouter, createWebHistory } from 'vue-router'\n\nimport store from '../store'\n\nimport Home from '../views/Home.vue'\n\nimport Product from '../views/Product.vue'\nimport Category from '../views/Category.vue'\nimport Search from '../views/Search.vue'\nimport Cart from '../views/Cart.vue'\nimport SignUp from '../views/SignUp.vue'\nimport LogIn from '../views/LogIn.vue'\nimport MyAccount from '../views/MyAccount.vue'\nimport Checkout from '../views/Checkout.vue'\nimport Success from '../views/Success.vue'\n\nconst routes = [\n  {\n    path: '/',\n    name: 'Home',\n    component: Home\n  },\n  {\n    path: '/about',\n    name: 'About',\n    component: () => import('../views/About.vue')\n  },\n  {\n    path: '/sign-up',\n    name: 'SignUp',\n    component: SignUp\n  },\n  {\n    path: '/log-in',\n    name: 'LogIn',\n    component: LogIn\n  },\n  {\n    path: '/my-account',\n    name: 'MyAccount',\n    component: MyAccount,\n    meta: {\n        requireLogin: true\n    }\n  },\n  {\n    path: '/search',\n    name: 'Search',\n    component: Search\n  },\n  {\n    path: '/cart',\n    name: 'Cart',\n    component: Cart\n  },\n  {\n    path: '/cart/success',\n    name: 'Success',\n    component: Success\n  },\n  {\n    path: '/cart/checkout',\n    name: 'Checkout',\n    component: Checkout,\n    meta: {\n        requireLogin: true\n    }\n  },\n  {\n    path: '/:category_slug/:product_slug',\n    name: 'Product',\n    component: Product\n  },\n  {\n    path: '/:category_slug',\n    name: 'Category',\n    component: Category\n  }\n]\n\n// Создает кукис для каждого пользователя\nconst router = createRouter({\n  history: createWebHistory(process.env.BASE_URL),\n  routes\n})\n\n// Использует эти кукис для отображения нужной страницы\nrouter.beforeEach((to, from, next) => {\n  if (to.matched.some(record => record.meta.requireLogin) && !store.state.isAuthenticated) {\n    next({ name: 'LogIn', query: { to: to.path } });\n  } else {\n    next()\n  }\n})\n\nexport default router"]}]}